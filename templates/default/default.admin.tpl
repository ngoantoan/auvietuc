<style type="text/css">
	
.column{
	float:none;
	min-height:20px;
}
#table_modules td{
	border:1px solid #999999;
}
</style>
<!-- refer template.configuration.xml -->
<div style="width:650px; overflow:hidden;background-color:#fff; " id="mainContainer">
	<table cellpadding="0" cellspacing="0" border="0" width="650px" id="table_modules">
	<tbody>

        <tr align="center">
            <td colspan="2" style="height:50px;" valign="top" align="center">{HEADER}</td>
		</tr>
		<tr align="center">
			<td colspan="2" style="height:50px;" valign="top" align="center">{TOP}</td>
		</tr>
		<tr align="center">
			<td colspan="1" style="height:50px;" valign="top" align="left">{LEFT}</td>

			<td colspan="1" style="height:50px;" valign="top" align="right">{RIGHT}</td>
		</tr>
		<tr align="center">
			<td colspan="2" style="height:50px;" valign="top" align="center">{BOTTOM}</td>
		</tr>
		<tr align="center">
		<td colspan="2" style="height:50px;" valign="top" align="center">{FOOTER}</td>
        </tr>
	</tbody>
	</table>
</div>